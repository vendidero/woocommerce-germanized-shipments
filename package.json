    "@wordpress/style-engine": "^1.15.0",
    "@wordpress/url": "3.13.0",
    "@wordpress/wordcount": "3.13.0",
    "change-case": "^4.1.2",
    "classnames": "2.3.1",
    "config": "3.3.7",
    "dinero.js": "1.9.1",
    "use-debounce": "9.0.4",
    "wordpress-components": "npm:@wordpress/components@14.2.0",
    "react-number-format": "4.9.3"
  },
  "engines": {
    "node": "^16.13.0",
    "npm": "^8.0.0"
  }
